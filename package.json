        "@radix-ui/react-icons": "^1.3.0",
        "@radix-ui/react-slot": "^1.0.2",
        "body-parser": "^1.20.2",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "lexical": "^0.13.1",
        "lorem-ipsum": "^2.0.8",
        "nanoid": "^5.0.6",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
