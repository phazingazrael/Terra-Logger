{
  "name": "terra-logger",
  "private": true,
  "version": "0.6.4",
  "main": "out/main/main.js",
  "description": "Terra-Logger is a tool that simplifies the process of organizing and managing data from Azgaar's Fantasy Map Generator.",
  "descriptionFull": "Terra-Logger is a tool that simplifies the process of organizing and managing data from Azgaar's Fantasy Map Generator. With Terra-Logger, you can take an exported JSON file from Azgaar's generator and automatically generate organized individual Markdown files for each city, province, country, religious details, and more. This makes it easier to document and present your world-building project in a structured and easily readable format.",
  "author": {
    "name": "Cameren Dessert",
    "email": "phazedverse@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "build:all": "npm run build:win && npm run build:mac && npm run build:linux",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build": "electron-vite build",
    "changelog": "auto-changelog --prepend -p",
    "dev": "electron-vite dev -w",
    "dist": "electron-builder",
    "format": "prettier --write .",
    "licenses": "generate-license-file --input package.json --output ./out/licenses.txt --overwrite",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "preview": "electron-vite preview"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@lexical/react": "^0.13.1",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-builder-squirrel-windows": "^24.13.2",
    "electron-router-dom": "^1.0.5",
    "electron-updater": "^6.1.7",
    "express": "^4.18.3",
    "lexical": "^0.13.1",
    "lorem-ipsum": "^2.0.8",
    "nanoid": "^5.0.6",
    "nedb": "^1.8.0",
    "prop-types": "^15.8.1",
    "react-editext": "^6.0.0",
    "react-icons": "^5.0.1",
    "react-lines-ellipsis": "^0.15.4",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.19.9",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^29.1.0",
    "electron-builder": "^24.12.0",
    "electron-icon-maker": "^0.0.5",
    "electron-vite": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.1.7"
  }
}
